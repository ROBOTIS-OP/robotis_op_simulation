<?xml version="1.0" ?>
<launch>

   <param name="robot_description" textfile="$(find robotis_op_description)/urdf/robotis_op.urdf" />  

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="worlds/empty.world"/>
    <arg name="paused" value="true"/>
    
  </include>
  
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-file $(find robotis_op_description)/urdf/robotis_op.urdf -urdf -model robotis_op -z 0.3417" />




  <include file="$(find robotis_op_simulation_control)/launch/robotis_op_simulation_control.launch"/>








</launch>
