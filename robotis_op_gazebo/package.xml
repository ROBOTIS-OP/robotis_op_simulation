<?xml version="1.0"?>
<package>
  <name>robotis_op_gazebo</name>
  <version>0.1.0</version>
  <description>Provides Gazebo launch files to expose ROBOTIS OP model in ROS.  Based on the darwin_gazebo package by Philippe Capdepuy</description>

  <maintainer email="inna@robotis.com">Inna Tlisov (ROBOTIS)</maintainer>


  <license>BSD</license>
  <url type="website">http://wiki.ros.org/robotis_op_gazebo</url>


  <author email="pc@humarobotics.com">Philippe Capdepuy</author>
  <author email="kevin.daun@freenet.de">Kevin Daun</author>


  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>gazebo_ros</build_depend>
  <build_depend>rospy</build_depend>
  <build_depend>gazebo_ros_control</build_depend>
  <build_depend>sensor_msgs</build_depend>
  
  <run_depend>gazebo_ros</run_depend>
  <run_depend>rospy</run_depend>
  <run_depend>gazebo_ros_control</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>robotis_op_description</run_depend>
  <run_depend>hector_gazebo_plugins</run_depend>


</package>
